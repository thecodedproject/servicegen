package local

import (
	context "context"
	internal "github.com/thecodedproject/servicegen/example_basic_service/internal"
	resources "github.com/thecodedproject/servicegen/example_basic_service/resources"
)

type client struct {
	r resources.Resources
}

func (c *client) Ping(ctx context.Context) (string, error) {

	return internal.Ping(
		ctx,
		c.r,
	)
}

func (c *client) Echo(
	ctx context.Context,
	reqMessage string,
) (string, error) {

	return internal.Echo(
		ctx,
		c.r,
		reqMessage,
	)
}

func (c *client) UseManyTypes(
	ctx context.Context,
	one int32,
	two int64,
	three float64,
	four string,
	five []byte,
	six bool,
) (int32, int64, float64, string, []byte, bool, error) {

	return internal.UseManyTypes(
		ctx,
		c.r,
		one,
		two,
		three,
		four,
		five,
		six,
	)
}

